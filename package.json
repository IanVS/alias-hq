{
  "name": "alias-hq",
  "version": "4.0.0",
  "description": "Manage a single set of folder aliases and convert on-the-fly to other formats",
  "main": "src/index.js",
  "bin": "bin/alias-hq",
  "files": [
    "src/*",
    "cli/*",
    "bin/*"
  ],
  "scripts": {
    "cli": "node cli",
    "test": "jest --verbose",
    "test:plugins": "jest --watch -f plugins.spec.js -t 'available plugins:'",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davestewart/alias-hq.git"
  },
  "keywords": [
    "paths",
    "aliases",
    "typescript",
    "webstorm",
    "vscode",
    "rollup",
    "webpack",
    "jest"
  ],
  "author": "Dave Stewart",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davestewart/alias-hq/issues"
  },
  "homepage": "https://github.com/davestewart/alias-hq#readme",
  "devDependencies": {
    "jest": "^26.4.1"
  },
  "dependencies": {
    "inquirer": "^7.3.3"
  }
}
